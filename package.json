{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "all": "npm-run-all --parallel lite watch:scss",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
    "usemin": "usemin index.html -d dist -o dist/index.html --htmlmin true",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "cssmin": "^0.4.3",
    "grunt": "^1.4.1",
    "grunt-browser-sync": "^1.3.7",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-htmlmin": "^3.1.0",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^5.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.1.0",
    "grunt-usemin": "^0.1.13",
    "gulp": "^5.0.0",
    "gulp-sass": "^5.0.0",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.0",
    "time-grunt": "^2.0.0",
    "uglify-js": "^3.14.1",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.0.7"
  },
  "dependencies": {
    "bootstrap": "^5.0.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "imagemin": "^8.0.0",
    "imagemin-cli": "^8.0.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  }
}
